dialog main
  run greet
  run help
  run bye
dialog qna
  nlp
dialog greet
  inc count 2
  run ask_name
  call uppers user_name
  [user_name !== 'ADMIN'] say Hello {{ user_name }} {{ count }}
  [user_name === 'ADMIN'] say Admin rights granted
dialog help
  say This is the help
  say This is the second help
dialog bye
  say Bye user
